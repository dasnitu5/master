pipeline {
    agent any
	stages{
	stage('jira-installation') {
		steps {
            powershell '''
			$path="C:/Atlassian/atlassian-jira-software-8.2.1-standalone/conf/server.xml"
			(Get-Content $path)`
			-replace '(<Server port="8005")', '(<Server port="8015")'`
			-replace '(<Connector port="8080")', '(<Connector_port="8100")'|Set-Content $path
			'''
			
			}
			}
			}
			}
